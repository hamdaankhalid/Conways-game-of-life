{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "paths": {
      "*": [
        "src/main/*",
        "node_modules/*"
      ]
    }
  },
  "include": [
    "src/main/*"
  ],
  "lib": [
    "es2015"
  ]
}